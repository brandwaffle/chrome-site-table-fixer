{
  "name": "Chrome Site Table Fixer Extension",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "https://developer.chrome.com/*", "http://developer.chrome.com/*"
  ],
  "content_scripts": [ 
	{
  	"js" : ["js/table-fixer.js"],
		"matches" : [ "https://developer.chrome.com/*", "http://developer.chrome.com/*" ],
		"run_at" : "document_end",
		"all_frames" : true
	}]
}
